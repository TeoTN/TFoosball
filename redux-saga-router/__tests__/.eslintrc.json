{
  "parser": "babel-eslint",
  "env": {
    "jest": true
  },
  "extends": [
    "airbnb",
    "plugin:import/errors",
    "plugin:import/warnings"
  ],
  "settings": {
    "import/resolver": "node"
  },
  "rules": {
    "arrow-parens": 0,
    "no-underscore-dangle": 0,
    "import/no-extraneous-dependencies": 0,
    "react/forbid-prop-types": 0,
    "react/jsx-filename-extension": 0
  }
}
