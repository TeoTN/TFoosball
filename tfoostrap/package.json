{
  "name": "@tfoosball/tfoostrap",
  "version": "2.4.2",
  "description": "Bootstrap stylesheet package crafted for TFoosball",
  "main": "dist/index.css",
  "scripts": {
    "prepublish": "echo \">>>>> Prepublish\" && lessc index.less dist/index.css",
    "prepack": "echo \">>>>> Prepack\" &&lessc index.less dist/index.css",
    "prepare": "echo \">>>>> Prepare\" &&lessc index.less dist/index.css",
    "publish": "echo \">>>>> Publish\" &&lessc index.less dist/index.css",
    "build": "lessc index.less dist/index.css",
    "start": "npm-watch dist"
  },
  "author": "Piotr Stani√≥w",
  "license": "ISC",
  "dependencies": {
    "bootstrap": "3.3.7"
  },
  "devDependencies": {
    "less": "3.0.4",
    "npm-watch": "0.1.9"
  },
  "watch": {
    "dist": {
      "patterns": [
        "*"
      ],
      "extensions": "less",
      "quiet": true
    }
  }
}
